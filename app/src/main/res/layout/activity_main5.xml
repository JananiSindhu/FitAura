<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity5">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:orientation="horizontal"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/mxx"
                android:layout_margin="10dp"
                android:padding="6dp"
                android:gravity="center"
                android:elevation="10dp"
                >

                <ImageButton
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_gravity="center"
                    android:src="@drawable/settings3"
                    tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck"></ImageButton>
            </LinearLayout>

            <TextView
                android:layout_width="60dp"
                android:layout_height="30dp"
                android:text="Track"
                android:textSize="20dp"
                android:fontFamily="@font/poppins"
                android:textColor="#343a6b"
                android:layout_marginTop="15dp"
                android:layout_marginLeft="80dp"
                ></TextView>
        </LinearLayout>

        <Button
            android:layout_width="100dp"
            android:layout_height="35dp"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/mxxx"
            android:elevation="10dp"
            android:fontFamily="@font/poppins"
            android:text="my goal"
            android:textColor="@color/white"
            android:textSize="13dp"
            tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>
    </LinearLayout>
    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/mxx"
        android:padding="8dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:elevation="10dp"
        >
        <LinearLayout
            android:layout_width="410dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins"
                        android:text="Rewards Points"
                        android:textColor="#575d62"
                        android:textSize="12dp"
                        tools:ignore="TextSizeCheck,TextSizeCheck,TextSizeCheck,TextSizeCheck"></TextView>

                    <TextView
                        android:layout_width="35dp"
                        android:layout_height="40dp"
                        android:textSize="30dp"
                        android:fontFamily="@font/poppins"
                        android:text="0"
                        android:textColor="#343a6b"
                        android:layout_marginLeft="20dp"
                        android:gravity="center"
                        ></TextView>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >

                    <TextView
                        android:layout_width="130dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins"
                        android:text="Heart Attack Risk "
                        android:textColor="#575d62"
                        android:textSize="12dp"
                        tools:ignore="TextSizeCheck"></TextView>

                    <TextView
                        android:layout_width="35dp"
                        android:layout_height="30dp"
                        android:textSize="30dp"
                        android:fontFamily="@font/poppins"
                        android:text="-"
                        android:textColor="#343a6b"
                        android:layout_marginLeft="30dp"
                        ></TextView>
                </LinearLayout>
            </LinearLayout>

            <ImageButton
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="15dp"
                android:src="@drawable/letter3"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"></ImageButton>
        </LinearLayout>

        <View
            android:layout_width="350dp"
            android:layout_height="4dp"
            android:background="#9e9da3"
            android:layout_marginLeft="20dp"
            ></View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >

            <TextView
                android:layout_width="40dp"
                android:layout_height="20dp"
                android:layout_marginLeft="20dp"
                android:fontFamily="@font/poppins"
                android:text="April"
                android:textColor="#575d62"
                android:textSize="12dp"
                tools:ignore="TextSizeCheck"></TextView>

            <TextView
                android:layout_width="70dp"
                android:layout_height="20dp"
                android:layout_marginLeft="250dp"
                android:fontFamily="@font/poppins"
                android:text="0/2000pts"
                android:textColor="#575d62"
                android:textSize="12dp"
                tools:ignore="TextSizeCheck"></TextView>
        </LinearLayout>
        <TextView
            android:layout_width="400dp"
            android:layout_height="25dp"
            android:textSize="20dp"
            android:fontFamily="@font/poppins"
            android:text="Get the most out of FitAura!"
            android:textColor="#343a6b"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            ></TextView>

        <TextView
            android:layout_width="310dp"
            android:layout_height="50dp"
            android:layout_marginLeft="20dp"
            android:fontFamily="@font/poppins"
            android:text="Let's connect your device and start your journey to health!"
            android:textColor="#575d62"
            android:textSize="15dp"
            tools:ignore="TextSizeCheck"></TextView>

        <Button
            android:layout_width="300dp"
            android:layout_height="40dp"
            android:layout_marginLeft="30dp"
            android:background="@drawable/mxxx"
            android:fontFamily="@font/poppins"
            android:text="Connect now"
            android:textColor="@color/white"
            android:textSize="18dp"
            android:id="@+id/button20"
            tools:ignore="TouchTargetSizeCheck"></Button>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            android:layout_width="100dp"
            android:layout_height="25dp"
            android:textSize="20dp"
            android:fontFamily="@font/poppins"
            android:text="Metrics"
            android:textColor="#343a6b"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            ></TextView>

        <TextView
            android:layout_width="150dp"
            android:layout_height="25dp"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/poppins"
            android:text="View Analysis"
            android:textColor="#42a5e8"
            android:textSize="15dp"
            tools:ignore="TextSizeCheck"></TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:background="@drawable/mxx"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:elevation="10dp"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:background="@drawable/mx"
                    android:gravity="center"
                    android:padding="10dp"
                    android:layout_marginTop="10dp"
                    >
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/run"
                        ></ImageView></LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    >

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins"
                        android:text="Activity"
                        android:textColor="#343a6b"
                        android:textSize="15dp"
                        tools:ignore="TextSizeCheck"></TextView>

                    <TextView
                        android:layout_width="60dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="20dp"
                        android:fontFamily="@font/poppins"
                        android:text="-/-mins"
                        android:textColor="#575d62"
                        android:textSize="12dp"
                        tools:ignore="TextSizeCheck"></TextView>
                </LinearLayout>
            </LinearLayout>

            <Button
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/mxxx"
                android:fontFamily="@font/poppins"
                android:text="Set up"
                android:textColor="@color/white"
                android:textSize="13dp"
                android:id="@+id/button5"
                tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:background="@drawable/mxx"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:elevation="10dp"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:background="@drawable/mx"
                    android:gravity="center"
                    android:padding="10dp"
                    android:layout_marginTop="10dp"
                    >
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/bp"
                        ></ImageView></LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    >

                    <TextView
                        android:layout_width="130dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins"
                        android:text="Blood Pressure"
                        android:textColor="#343a6b"
                        android:textSize="15dp"
                        tools:ignore="TextSizeCheck"></TextView>

                    <TextView
                        android:layout_width="65dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="20dp"
                        android:fontFamily="@font/poppins"
                        android:text="-/-mmHg"
                        android:textColor="#575d62"
                        android:textSize="12dp"
                        tools:ignore="TextSizeCheck"></TextView>
                </LinearLayout>
            </LinearLayout>

            <Button
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/mxxx"
                android:fontFamily="@font/poppins"
                android:text="Set up"
                android:textColor="@color/white"
                android:textSize="13dp"
                android:id="@+id/button21"
                tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:background="@drawable/mxx"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:elevation="10dp"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:background="@drawable/mx"
                    android:gravity="center"
                    android:padding="10dp"
                    android:layout_marginTop="10dp"
                    >
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/weight"
                        ></ImageView></LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    >

                    <TextView
                        android:layout_width="100dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins"
                        android:text="Weight"
                        android:textColor="#343a6b"
                        android:textSize="15dp"
                        tools:ignore="TextSizeCheck"></TextView>

                    <TextView
                        android:layout_width="60dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="20dp"
                        android:fontFamily="@font/poppins"
                        android:text="-/-Kg"
                        android:textColor="#575d62"
                        android:textSize="12dp"
                        tools:ignore="TextSizeCheck,TextSizeCheck,TextSizeCheck"></TextView>
                </LinearLayout>
            </LinearLayout>

            <Button
                android:layout_width="100dp"
                android:layout_height="35dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/mxxx"
                android:fontFamily="@font/poppins"
                android:text="Set up"
                android:textColor="@color/white"
                android:textSize="13dp"
                tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="380dp"
        android:layout_height="80dp"
        android:orientation="vertical"
        android:background="@drawable/mxx"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:elevation="10dp"
        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:orientation="horizontal"
                    android:background="@drawable/mx"
                    android:gravity="center"
                    android:padding="10dp"
                    android:layout_marginTop="10dp"
                    >
                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@drawable/blooddrop"
                        ></ImageView></LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginTop="5dp"
                    >

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/poppins"
                        android:text="Blood Glucose"
                        android:textColor="#343a6b"
                        android:textSize="15dp"
                        tools:ignore="TextSizeCheck"></TextView>

                    <TextView
                        android:layout_width="65dp"
                        android:layout_height="25dp"
                        android:layout_marginLeft="20dp"
                        android:fontFamily="@font/poppins"
                        android:text="-/-mg/dL"
                        android:textColor="#575d62"
                        android:textSize="12dp"
                        tools:ignore="TextSizeCheck"></TextView>
                </LinearLayout>
            </LinearLayout>

            <Button
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/mxxx"
                android:fontFamily="@font/poppins"
                android:text="Set up"
                android:textColor="@color/white"
                android:textSize="13dp"
                tools:ignore="TextSizeCheck,TouchTargetSizeCheck"></Button>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>